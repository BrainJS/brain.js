{
  "env": {},
  "extends": ["airbnb", "prettier"],
  "globals": {},
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "root": true,
  "rules": {
    "class-methods-use-this": "off",
    "linebreak-style": 0,
    "no-continue": 0,
    "no-multi-assign": "off",
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-prototype-builtins": 0,
    "no-underscore-dangle": 0,
    "semi": 0
  }
}
