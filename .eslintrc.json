{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "airbnb", "prettier"],
  "globals": {},
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "root": true,
  "rules": {
    "class-methods-use-this": "off",
    "linebreak-style": 0,
    "no-continue": 0,
    "no-multi-assign": "off",
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-prototype-builtins": 0,
    "no-underscore-dangle": 0,
    "semi": 1
  }
}
